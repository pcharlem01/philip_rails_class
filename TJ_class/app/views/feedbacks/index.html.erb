<h1>Listing feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Rate service from 1 to 10</th>
      <th>Name of your representative</th>
      <th>Would you use us again</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.rate_service_from_1_to_10 %></td>
        <td><%= feedback.name_of_your_representative %></td>
        <td><%= feedback.would_you_use_us_again %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
